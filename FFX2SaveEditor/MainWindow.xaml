    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:Themes="clr-namespace:Microsoft.Windows.Themes;assembly=PresentationFramework.Aero" x:Class="FFX2SaveEditor.MainWindow"
    xmlns:local="clr-namespace:FFX2SaveEditor"
    Title="FFX-2 Save Editor"
    Name="ffx2MainWindow"
    FontFamily="Helvetica Neue" Icon="Resources/tonberry.gif" WindowStartupLocation="CenterScreen">
